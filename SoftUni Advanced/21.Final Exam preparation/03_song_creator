def add_songs(*typles):
    songs = {}

    for t in typles:
        song, lyrics = t

        if song not in songs:
            songs[song] = []

        songs[song].extend(lyrics)

    output= []

    for s_title, s_lyrics in songs.items():
        output.append("- " + s_title)
        output.extend(s_lyrics)

    return "\n".join(output)